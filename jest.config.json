{
  "verbose": true,
  "testPathIgnorePatterns": ["/node_modules", "/.next", "/.public"],
  "setupFilesAfterEnv": ["<rootDir>/src/tests/setupTests.ts"],
  "transform": {
    "^.+\\.(js|jsx|ts|tsx)$": ["<rootDir>/node_modules/babel-jest"]
  },
  "testEnvironment": "jsdom",
  "moduleNameMapper": {
    "\\.(css|scss|sass)$": "identity-obj-proxy"
  }
}
